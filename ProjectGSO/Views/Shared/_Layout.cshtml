                        </ul>
                        <a class='menu-trigger'>
                            <span>Menu</span>
                        </a>
                        <!-- ***** Menu End ***** -->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- ***** Header Area End ***** -->

<div class="container" style="margin-left: 0px; margin-right: 0px">
    <main role="main" class="pb-3">
        @RenderBody()
